    <Name>availableFunds</Name>
    <ShortName>$$</ShortName>
    <Description>Cash money.</Description>
    <PropertyName>AvailableFunds</PropertyName>
  </Identifier>

  <Identifier>
    <Name>citySize</Name>
    <ShortName>cs</ShortName>
    <Description>Total city size (residential, commercial, industrial).</Description>
    <PropertyName>CitySize</PropertyName>
  </Identifier>

  <Identifier>
    <Name>citySizeArcologies</Name>
    <ShortName>csa</ShortName>
    <Description>Total city size for arcologies only.</Description>
    <PropertyName>CitySizeArcology</PropertyName>
  </Identifier>

  <Identifier>
    <Name>citySizeNoArcologies</Name>
    <ShortName>csa</ShortName>
    <Description>Total city size not counting arcologies.</Description>
    <PropertyName>CitySizeNoArcology</PropertyName>
  </Identifier>

  <Identifier>
    <Name>citySizeResidential</Name>
    <ShortName>csr</ShortName>
    <Description>Total residential size.</Description>
    <PropertyName>ResidentialSize</PropertyName>
  </Identifier>

  <Identifier>
    <Name>citySizeCommercial</Name>
    <ShortName>csc</ShortName>
    <Description>Total commercial size. (NOTE: This value is lower than it should be if Arcologies are present)</Description>
    <PropertyName>CommercialSize</PropertyName>
  </Identifier>

  <Identifier>
    <Name>citySizeIndustrial</Name>
    <ShortName>csi</ShortName>
    <Description>Total industrial size. (NOTE: This value is lower than it should be if Arcologies are present)</Description>
    <PropertyName>IndustrialSize</PropertyName>
  </Identifier>
    
  <Identifier>
    <Name>lifeExpectancy</Name>
    <ShortName>le</ShortName>
    <Description>Life expectacy for working-age sims. A measure of citywide health.</Description>
    <PropertyName>LifeExpectancy</PropertyName>
  </Identifier>

  <Identifier>
    <Name>educationQuotent</Name>
    <ShortName>eq</ShortName>
