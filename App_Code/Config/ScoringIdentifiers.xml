    <PropertyName>EducationQuotent</PropertyName>
  </Identifier>

  <Identifier>
    <Name>workforcePercentage</Name>
    <ShortName>wfp</ShortName>
    <Description>Percentage of sims aged 20-55 in your city.</Description>
    <PropertyName>WorkforcePercentage</PropertyName>
  </Identifier>

  <Identifier>
    <Name>yearOfFounding</Name>
    <ShortName>yf</ShortName>
    <Description>The year your city was founded (1900, 1950, 2000 or 2050).</Description>
    <PropertyName>YearOfFounding</PropertyName>
  </Identifier>

  <Identifier>
    <Name>daysSinceFounding</Name>
    <ShortName>dsf</ShortName>
